public class Main {
 
    public static void main(String[] args) throws IOException {
        
        Scanner sc = new Scanner(System.in);
 
        int n;
        double h, v, s;
        
        
        // n = numero do funcionario
        // h = nº de horas trabalhadas
        // v = valor do trabalho-hora
        // s = salário
        
        n = sc.nextInt();
        h = sc.nextDouble();
        v = sc.nextDouble();
        
        s = h * v;
        
        System.out.println("NUMBER = " + n);
        System.out.printf("SALARY = U$ %.2f%n", s);
        
        sc.close();
 
    }
 
}
